<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">    
<mapper namespace="com.goodee.everydoctor.location.LocationMapper">
	<!-- 선별검사소 지도찍기 -->
	<select id="findCoronaList" resultType="AgencyVO">
		SELECT * 
		FROM AGENCY
		WHERE AGENCYROLE = '선별진료소'
			  AND
			  (AGENCYADDR REGEXP '구로|서울|중랑구|은평구|동작구')
			  
	</select>
	<!-- 약국 지도찍기 -->
	<select id="findPharmacyList" resultType="AgencyVO">
		SELECT * 
		FROM AGENCY 
		WHERE AGENCYROLE='약국'
	</select>
</mapper>